<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FDE9E9"
    tools:context=".MyAppoinment">

    <TextView
        android:id="@+id/textView"
        android:layout_width="265dp"
        android:layout_height="65dp"
        android:text="..My Appoinment.."
        android:textColor="#E65791"
        android:textSize="30sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.115" />

    <ImageView
        android:id="@+id/icon"
        android:layout_width="111dp"
        android:layout_height="62dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.023"
        app:srcCompat="@drawable/tran" />

    <CalendarView
        android:id="@+id/calendar"
        android:layout_width="370dp"
        android:layout_height="226dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.365"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.229" />

    <SearchView
        android:id="@+id/search"
        android:layout_width="65dp"
        android:layout_height="26dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.098"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

    </SearchView>

    <TextView
        android:id="@+id/Upcome"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="220dp"
        android:text="Upcoming"
        android:textColor="#1DAC3C"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/past"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Past"
        android:textColor="#33691E"
        android:textSize="20"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.597"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="180dp"
        android:layout_height="258dp"
        android:background="#ECF151"
        tools:layout_editor_absoluteX="15dp"
        tools:layout_editor_absoluteY="379dp" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="178dp"
        android:layout_height="261dp"
        android:background="#B0D683"
        android:text="TextView"
        android:textColor="#ACD178"
        tools:layout_editor_absoluteX="221dp"
        tools:layout_editor_absoluteY="379dp" />

    <Button
        android:id="@+id/AddApp"
        android:layout_width="134dp"
        android:layout_height="61dp"
        android:layout_marginEnd="168dp"
        android:text="Delete Appoinment"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.532"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.967" />

    <ImageView
        android:id="@+id/imageView10"
        android:layout_width="46dp"
        android:layout_height="72dp"
        app:srcCompat="@drawable/delete"
        tools:layout_editor_absoluteX="12dp"
        tools:layout_editor_absoluteY="637dp" />

    <ImageView
        android:id="@+id/imageView14"
        android:layout_width="42dp"
        android:layout_height="56dp"
        app:srcCompat="@drawable/edit"
        tools:layout_editor_absoluteX="221dp"
        tools:layout_editor_absoluteY="648dp" />

    <Button
        android:id="@+id/button5"
        android:layout_width="128dp"
        android:layout_height="59dp"
        android:text="Add Appoinment"
        tools:layout_editor_absoluteX="263dp"
        tools:layout_editor_absoluteY="648dp" />

    <TextView
        android:id="@+id/1"
        android:layout_width="149dp"
        android:layout_height="19dp"
        android:text="2020/9/2  facial"
        tools:layout_editor_absoluteX="17dp"
        tools:layout_editor_absoluteY="405dp" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="195dp"
        android:layout_height="44dp"
        android:text="2020/11/8   Colouring hair"
        tools:layout_editor_absoluteX="17dp"
        tools:layout_editor_absoluteY="465dp" />

    <TextView
        android:id="@+id/2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="2019/4/5      Treading"
        tools:layout_editor_absoluteX="221dp"
        tools:layout_editor_absoluteY="405dp" />

    <TextView
        android:id="@+id/3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="2019/2/19      cutting hair"
        tools:layout_editor_absoluteX="221dp"
        tools:layout_editor_absoluteY="465dp" />

    <TextView
        android:id="@+id/4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="20201/7     Nail Art"
        tools:layout_editor_absoluteX="17dp"
        tools:layout_editor_absoluteY="524dp" />

</androidx.constraintlayout.widget.ConstraintLayout>